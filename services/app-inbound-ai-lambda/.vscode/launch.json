{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (dotnet tools lambda)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${env:USERPROFILE}\\.dotnet\\tools\\dotnet-lambda-test-tool-8.0.exe",
            "args": [],
            "cwd": "${workspaceFolder}\\src\\MyLambdaProject",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}\\src\\MyLambdaProject\\bin\\Debug\\net8.0\\MyLambdaProject.dll",
            "args": [],
            "cwd": "${workspaceFolder}\\src\\MyLambdaProject",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }
    ]
}
