{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "app-process-bestai-lambda (dotnet tools lambda)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${env:USERPROFILE}\\.dotnet\\tools\\dotnet-lambda-test-tool-8.0.exe",
            "args": [],
            "cwd": "${workspaceFolder}\\src\\MyLambdaProject",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
            }
        }
    ]
}
